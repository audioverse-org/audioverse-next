    "next-sitemap": "^2.5.10",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "tsconfig-paths-jest": "^0.0.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "type-fest": "^2.12.0",
    "typescript": "^4.4.2"
  },
  "nextBundleAnalysis": {
    "budget": 230400,
    "budgetPercentIncreaseRed": 5,
    "showDetails": true
  }
}
